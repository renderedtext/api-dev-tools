#<%= @entity.capitalize %>

<% @resources && @resources.each do |resource| %>
##<%= resource.raw["parentUrl"] %><%= resource.raw["relativeUri"] %>

  <% resource.methods.each do |method| %>
###<%= method.raw["method"] %>

    <%= method.raw["description"].to_s.split("\n").first %>

    Responses:
    <% method.responses.each do |response| %>
####<%= response.raw["code"] %>
      <% body = (response.raw["body"] && response.raw["body"].first).to_h %>

      <%= body["displayName"] %>
      <%= body["key"] %>

      Properties:
      <% response.properties.each do |property| %>
        <%= property.raw["displayName"] %>(<%= property.raw["type"] %>):
        Required: <%= property.raw["required"] %>
      <% end %>

    <% end %>

  <% end %>

<% end %>
